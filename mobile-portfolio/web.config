<?xml version="1.0"?>
<configuration>
  <appSettings/>
  <connectionStrings/>
  <system.web>
    <compilation debug="false">
    </compilation>
    <authentication mode="Windows" />
  </system.web>

  <location path="css">
    <system.webServer>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.02:00:00" cacheControlCustom="public" />
      </staticContent>
    </system.webServer>
  </location>
  <location path="img">
    <system.webServer>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.00:10:00" cacheControlCustom="public" />
      </staticContent>
    </system.webServer>
  </location>
  <location path="js">
    <system.webServer>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.02:00:00" cacheControlCustom="private" />
      </staticContent>
    </system.webServer>
  </location>
  <location path="views/css">
    <system.webServer>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.00:10:00" cacheControlCustom="public" />
      </staticContent>
    </system.webServer>
  </location>
  <location path="views/images">
    <system.webServer>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.00:10:00" cacheControlCustom="public" />
      </staticContent>
    </system.webServer>
  </location>
   <location path="views/js">
    <system.webServer>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.02:00:00" cacheControlCustom="private" />
      </staticContent>
    </system.webServer>
  </location>

</configuration>